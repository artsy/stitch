{
  "name": "@artsy/stitch",
  "version": "1.5.1",
  "description": "Helps your Component and Template dependencies peacefully coexist",
  "main": "./dist/index.js",
  "repository": "git@github.com:artsy/stitch.git",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean:build && babel src --ignore src/__tests__ --out-dir dist",
    "clean:build": "rm -rf dist",
    "coverage": "jest --coverage",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "setupFiles": [
      "./.test-setup"
    ],
    "transform": {
      ".(js|jsx)": "babel-jest"
    }
  },
  "authors": [
    "Christopher Pappas <christopher@artsymail.com>",
    "Alan Johnson <alan@artsymail.com>",
    "Artsy <https://artsy.net>"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.46",
    "consolidate": "^0.14.5",
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16",
    "styled-components": "^3"
  },
  "devDependencies": {
    "@artsy/express-reloadable": "^1.0.6",
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/preset-stage-3": "^7.0.0-beta.46",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "ejs": "^2.5.7",
    "eslint": "^4.5.0",
    "eslint-config-prettier-standard": "^1.0.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "handlebars": "^4.0.10",
    "jade": "^1.11.0",
    "jest": "^22.4.2",
    "prettier": "^1.10.2",
    "pug": "^2.0.0-rc.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.2.6"
  }
}
